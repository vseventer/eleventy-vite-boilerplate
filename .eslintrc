{
    "root": true,
    "extends": [
      "airbnb-base",
      "plugin:react/recommended"
    ],
    "parserOptions": { "ecmaVersion": 2021 },
    "reportUnusedDisableDirectives": true,
    "settings": {
      "react": { "version": "detect" }
    },
    "rules": {
      "react/prop-types": 0,
      "max-len": [2, 100, { "ignoreUrls": true }]
    }
  }
