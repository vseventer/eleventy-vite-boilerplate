{
  "name": "eleventy-vite-boilerplate",
  "version": "0.1.0",
  "description": "Eleventy and Vite boilerplate.",
  "main": "eleventy.config.js",
  "scripts": {
    "clean": "rimraf dist/",
    "lint:css": "stylelint styles/",
    "lint:js": "eslint '*.js' src/ --ext js,jsx",
    "lint": "npm run lint:css && npm run lint:js",
    "11ty": "eleventy --config eleventy.config.js",
    "dev": "npm run 11ty -- --serve --incremental",
    "start": "npm run dev",
    "prebuild": "npm run clean",
    "build": "npm run 11ty"
  },
  "dependencies": {
    "@11ty/eleventy": "^1.0.0",
    "@11ty/eleventy-img": "^2.0.1",
    "@11ty/eleventy-navigation": "^0.3.3",
    "@slinkity/renderer-react": "^1.0.0",
    "@tailwindcss/nesting": "^0.0.0-insiders.565cd3e",
    "@tailwindcss/typography": "^0.5.2",
    "postcss": "^8.4.14",
    "postcss-import": "^14.1.0",
    "postcss-reporter": "^7.0.5",
    "postcss-url": "^10.1.3",
    "react": "^17.0.2",
    "slinkity": "^0.8.3",
    "stylelint": "^14.9.1",
    "tailwindcss": "^3.1.4",
    "vite-plugin-eslint": "^1.6.1",
    "vite-plugin-imagemin": "^0.6.1"
  },
  "devDependencies": {
    "eslint": "^8.19.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-react": "^7.30.1",
    "rimraf": "^3.0.2",
    "stylelint-config-recommended": "^8.0.0",
    "stylelint-no-unsupported-browser-features": "^5.0.3"
  },
  "engines": {
    "node": ">=14.17"
  },
  "private": true
}
